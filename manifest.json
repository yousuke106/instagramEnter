{
  "manifest_version": 3,
  "name": "Instagram Ctrl+Enter Submit",
  "version": "1.0.0",
  "description": "Submit Instagram comments with Ctrl+Enter instead of Enter",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.instagram.com/*"],
      "js": ["src/content/content.js"],
      "run_at": "document_end"
    }
  ]
}